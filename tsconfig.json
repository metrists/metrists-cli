{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "lib": ["es2017"],
    "module": "commonjs",
    "target": "es6",
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": false,
    "useUnknownInCatchVariables": false,
    "types": ["node"],
    "resolveJsonModule": true
  },
  "include": ["actions", "bin", "commands", "lib", "package.json"],
  "exclude": ["node_modules", "e2e"]
}
