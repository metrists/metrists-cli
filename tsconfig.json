{
  "compilerOptions": {
    "outDir": "dist",
    "declaration": true,
    "lib": ["es2017"],
    "module": "commonjs",
    "target": "es6",
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": false,
    "useUnknownInCatchVariables": false,
    "types": ["node"],
    "resolveJsonModule": true
  },
  "exclude": ["e2e" , ".git" , ".circleci" , ".eslint.json" , ".prettierrc" , "nodemon.json"]
}
